<h1>Bonlivraison</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Numero</th>
            <td>{{ entity.numero }}</td>
        </tr>
        <tr>
            <th>Dateliv</th>
            <td>{{ entity.dateLiv|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Numbc</th>
            <td>{{ entity.numBc }}</td>
        </tr>
        <tr>
            <th>Objet</th>
            <td>{{ entity.objet }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('bonlivraison') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('bonlivraison_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('bonlivraison_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
