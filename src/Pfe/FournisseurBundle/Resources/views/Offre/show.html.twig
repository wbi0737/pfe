<h1>Offre</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Createdat</th>
            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Expiredat</th>
            <td>{{ entity.expiredAt|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Designation</th>
            <td>{{ entity.designation }}</td>
        </tr>
        <tr>
            <th>Userid</th>
            <td>{{ entity.userId }}</td>
        </tr>
        <tr>
            <th>Cahiercharge</th>
            <td>{{ entity.cahierCharge }}</td>
        </tr>
        <tr>
            <th>Code</th>
            <td>{{ entity.code }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('offre') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('offre_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('offre_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
